{
  "ConnectionStrings": {
    "MySql": "{mysql-conn}",
    "Redis": "{redis-conn}"
  },
  "Mongo": {
    "ConnectionString": "mongodb://{mongodb-conn}",
    "Database": "blog"
  },
  "CorsOrigins": "http://demo.com,http://blog.admin.memoyu.com,http://localhost:11011,http://localhost:11012",

  "Authorization": {
    "Jwt": {
      "Secret": "{jwt-secret}",
      "Issuer": "memoyu",
      "Audience": "core.me",
      "ExpiryInMin": 720
    },
    "Qiniu": {
      "AK": "{qiniu-ak}",
      "SK": "{qiniu-sk}",
      "Bucket": "demo",
      "Host": "http://oss.demo.com/",
      "UseHttps": false
    }
  }
}
